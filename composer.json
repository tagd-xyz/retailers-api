{
    "name": "laravel/laravel",
    "type": "project",
    "description": "Tagd Affiliates API",
    "keywords": ["tagd", "laravel"],
    "license": "MIT",
    "repositories": {
        "gitlab.totallydev.com": {
            "type": "composer",
            "url": "https://gitlab.totallydev.com/api/v4/group/1406/-/packages/composer/packages.json"
        }
     },
    "require": {
        "php": "^8.1",
        "fakerphp/faker": "^1.9.1",
        "firebase/php-jwt": "^6.4",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^9.10",
        "laravel/pint": "^1.2",
        "laravel/sanctum": "^2.14.1",
        "laravel/tinker": "^2.7",
        "tagd/core": "^0.0"
    },
    "require-dev": {
        "laravel/sail": "^1.0.1",
        "laravel/telescope": "^4.8",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Tagd\\Core\\" : "vendor/tagd/core/src/code/",
            "Tagd\\Core\\Database\\Seeders\\": "vendor/tagd/core/src/database/seeders/",
            "Tagd\\Core\\Database\\Factories\\": "vendor/tagd/core/src/database/factories/",
            "Tagd\\Core\\Tests\\": "vendor/tagd/core/tests/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Tagd\\Core\\" : "/packages/core/src/code/",
            "Tagd\\Core\\Database\\Seeders\\": "/packages/core/src/database/seeders/",
            "Tagd\\Core\\Database\\Factories\\": "/packages/core/src/database/factories/",
            "Tagd\\Core\\Tests\\": "/packages/core/tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "pre-autoload-dump": [
            "[ -d \"/packages/core\" ] && rm -rf vendor/tagd/core || echo \"using vendor tagd/core\""
        ],
        "test": [
            "vendor/bin/phpunit"
        ],
        "pint": [
            "vendor/bin/pint"
        ],
        "pint_check": [
            "vendor/bin/pint --test"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "process-timeout": 600,
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "gitlab-domains": [
            "gitlab.totallydev.com"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
